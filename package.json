{
  "name": "angular-exemplify",
  "version": "1.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hjalmers/angular-exemplify.git"
  },
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "clean": "rimraf exemplify \"index.@(js|js.map|d.ts|metadata.json|ngsummary.json)\"",
    "clean:factory": "rimraf \"exemplify/**/*.ngfactory.ts\"",
    "copy:css": "copyfiles -f src/exemplify/css/exemplify.css exemplify/css",
    "prebuild": "npm run -s clean",
    "build": "ngc -p src/tsconfig-aot.json && npm run copy:css",
    "postbuild": "npm run -s clean:factory",
    "prepublish": "npm run -s build",
    "postpublish": "npm run -s clean"
  },
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "index.js",
  "devDependencies": {
    "@angular/cli": "^1.0.0-rc.1",
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/compiler-cli": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.60",
    "bootstrap": "^4.0.0-alpha.6",
    "codelyzer": "~2.0.0",
    "copyfiles": "^1.0.0",
    "core-js": "^2.4.1",
    "jasmine-core": "2.5.2",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prismjs": "^1.6.0",
    "protractor": "~5.1.0",
    "rimraf": "^2.5.4",
    "rxjs": "^5.1.0",
    "ts-helpers": "^1.1.1",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "~2.0.3",
    "zone.js": "^0.7.6"
  }
}
